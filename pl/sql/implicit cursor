SET SERVEROUTPUT ON;
DECLARE
    total NUMBER;
BEGIN
    UPDATE emptbl SET salary = salary * 1.1 WHERE ename LIKE 'A%';
    IF SQL%NOTFOUND THEN
        dbms_output.put_line('No rows updated');
    ELSIF SQL%FOUND THEN
        total := SQL%ROWCOUNT;
        dbms_output.put_line('Total ' || total || ' emptbl rows updated.');
    END IF;
END;
/