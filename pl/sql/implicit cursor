SET SERVEROUTPUT ON;

DECLARE
    E_ID EMPTBL.EMPID%TYPE;
    E_NAME EMPTBL.ENAME%TYPE;
    E_SALARY EMPTBL.SALARY%TYPE;

    CURSOR EMP IS 
        SELECT EMPID, ENAME, SALARY 
        FROM EMPTBL
        WHERE ENAME LIKE 'A%';
BEGIN
    OPEN EMP;
    LOOP
        FETCH EMP INTO E_ID, E_NAME, E_SALARY;
        EXIT WHEN EMP%NOTFOUND;
        DBMS_OUTPUT.PUT_LINE(E_ID || ' ' || E_NAME || ' ' || E_SALARY);
    END LOOP;
    CLOSE EMP;
END;
/
